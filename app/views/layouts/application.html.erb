<!DOCTYPE html>
<html>
<head>
  <title>Mini Booker</title>
  <%= stylesheet_link_tag    "bootstrap/css/bootstrap" %>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="row-fluid">
  <div class="span12 header"><a href="/"><img src="/images/logo_head.jpg"></a></div>
  <div class="row-fluid">
    <div class="span2">
      <%= render "layouts/menu" %><br>

      <%= render "layouts/user_info" %><br>

      <%= render "layouts/blitz_info" %>
    </div>
    <div class="span10">
      <%#= show_flash %>

      <%= yield %>
    </div>
  </div>
</div>
</body>
</html>
